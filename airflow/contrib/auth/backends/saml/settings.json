{
  "strict": true,
  "sp": {
    "entityId": "https://airflow.devops.xiaohongshu.com",
    "assertionConsumerService": {
      "url": "https://airflow.devops.xiaohongshu.com/admin/airflow/saml",
      "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-POST"
    },
    "singleLogoutService": {
      "url": "https://airflow.devops.xiaohongshu.com/api/saml/logout",
      "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
    },
    "NameIDFormat": "urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified",
    "x509cert": "{{data_platform_cert}}",
    "privateKey": "{{data_platform_key}}"
  },
  "idp": {
    "entityId": "http://sso.devops.xiaohongshu.com/adfs/services/trust",
    "singleSignOnService": {
      "url": "https://sso.devops.xiaohongshu.com/adfs/ls",
      "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
    },
    "singleLogoutService": {
      "url": "https://sso.devops.xiaohongshu.com/adfs/ls",
      "binding": "urn:oasis:names:tc:SAML:2.0:bindings:HTTP-Redirect"
    },
    "x509cert": ""
  }
}
